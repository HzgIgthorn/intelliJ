<thlogic>
    <attr sel="#headId" th:replace="~{fragments/head :: headFragment}"/>
    <attr sel="#game-title" th:text="#{game.title}"/>
    <attr sel="#game-over" th:text="#{game.over}"/>
    <attr sel="#result-message" th:text="${__${T(academy.learnprogramming.util.AttributeNames).RESULT_MESSAGE}__}"/>
    <attr sel="#againlink" th:text="#{game.again}" th:href="@{__${T(academy.learnprogramming.util.GameMappings).RESTART}__}"/>
    <attr sel="#backlink" th:text="#{game.back}" th:href="@{__${T(academy.learnprogramming.util.GameMappings).HOME}__}"/>
    <attr sel="#footerId" th:replace="~{fragments/footer :: footFragment}"/>
</thlogic>